{
  "name": "ahao",
  "author": "阿豪阿卡阿甘",
  "version": "0.0.0",
  "description": "阿豪阿卡阿甘的项目",
  "main": "electron/index.ts",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:vue\" \"npm run dev:electron\"",
    "dev:vue": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env NODE_ENV=dev electron .",
    "server": "concurrently -k \"tsc server/index.ts\" \"node server/index\"",
    "build": "npm run build:vue && npm run build:electron",
    "build:vue": "run-p type-check build-only",
    "build:electron": "electron-builder",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "build": {
    "productName": "ahao",
    "copyright": "Copyright @ 2022 dong",
    "directories": {
      "output": "dist-electron"
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.68",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "express": "^4.18.2",
    "multiparty": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "qs": "^6.11.0",
    "spark-md5": "^3.0.2",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue-tsc": "^1.0.8",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@types/express": "^4.17.14"
  }
}
